    <bpmn:sequenceFlow id="task_send_acceptance_pack-end_accepted" sourceRef="task_send_acceptance_pack" targetRef="end_accepted" />
    <bpmn:sequenceFlow id="gateway_eligibility_decision-task_prepare_acceptance_pack" name="Applicant is eligible" sourceRef="gateway_eligibility_decision" targetRef="task_prepare_acceptance_pack" />
