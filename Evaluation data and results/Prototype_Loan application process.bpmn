      <bpmn:outgoing>task_appraise_property-parallel_assess_risk_and_appraise_property-join</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="task_assess_applicant_eligibility" name="Assess applicant&#39;s eligibility">
