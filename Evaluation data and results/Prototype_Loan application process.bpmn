      <bpmn:outgoing>task_assess_applicant_eligibility-gateway_eligibility_decision</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="gateway_eligibility_decision" name="Is applicant eligible?">
